[ignore]
.*/scripts/bench/.*
.*/build/.*
.*/.tempUserDataDir/.*

# These shims are copied into external projects:
.*/rollup/shims/facebook-www/.*
.*/rollup/shims/react-native/.*

.*/node_modules/@snyk/.*
.*/node_modules/y18n/.*
.*/node_modules/chrome-devtools-frontend/.*
.*/node_modules/devtools-timeline-model/.*
.*/node_modules/create-react-class/.*
.*/node_modules/react-native-web/.*
.*/__mocks__/.*
.*/__tests__/.*

# TODO: noop should get its own inlinedHostConfig entry
.*/packages/react-noop-renderer/.*

.*/packages/.*/forks/.*.dom.js
.*/packages/.*/forks/.*.dom-browser.js

[include]
./node_modules/
./packages/
./scripts/

[libs]
./node_modules/fbjs/flow/lib/dev.js
./scripts/flow/environment.js

[lints]
untyped-type-import=error

[options]
server.max_workers=4
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable

# Substituted by createFlowConfig.js:
module.name_mapper='ReactFiberHostConfig$' -> 'forks/ReactFiberHostConfig.native'
module.name_mapper='ReactServerStreamConfig$' -> 'forks/ReactServerStreamConfig.native'
module.name_mapper='ReactServerFormatConfig$' -> 'forks/ReactServerFormatConfig.native'
module.name_mapper='ReactFlightServerConfig$' -> 'forks/ReactFlightServerConfig.native'
module.name_mapper='ReactFlightClientHostConfig$' -> 'forks/ReactFlightClientHostConfig.native'

munge_underscores=false

suppress_type=$FlowIssue
suppress_type=$FlowFixMe
suppress_type=$FixMe
suppress_type=$FlowExpectedError

[version]
^0.129.0
